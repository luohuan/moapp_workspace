}
.mo-btn-small {
	width: max-content;
}
.textDefault {
	width: max-content;
	height: max-content;
}
button:after {
    border:none;
}
button {
	padding:0
}
.bgm-control {
	width:64rpx;
	height:64rpx;
	right:32rpx;
	top:48rpx;
	position:fixed;
	z-index:100;
	transform-origin: 50% 50%;
}
.bgmpaused {
  
}
.bgmrunning {
  animation-name:icon_animation;
  animation-duration: 4s;
  animation-iteration-count: infinite;
  animation-timing-function: linear;
}
.bgm-white {
	content: url(data:image/png;base64,);
}
.bgm-black {
	content: url(data:image/png;base64,);  	
}
.bgm-black{
  content: url(http://7u2r5e.com2.z0.glb.qiniucdn.com/music_note_big.png)
}
button,view,text,swiper,movable-view,input {
  box-sizing: border-box;
}




.label{
  display:block;
  width:600rpx;
  height:100rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
}

.label .left {
  width: 100rpx;
  height: 100%;
  background: #eeeeee;
  border-radius: 20rpx 0 0 20rpx;
  
}
.label .right{
  width: 500rpx;
  height: 100%;
  background: white;
  padding-left: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-radius: 0 20rpx  20rpx 0 ;
}
.label .right text {
  line-height:100rpx;
}
.fakeradio {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background: white;
  margin-left: 25rpx;
  margin-top:25rpx;
}
.fakeradio.choosed{
 
  background: #a23636;
}
@-webkit-keyframes icon_animation
{
  0%{
    -webkit-transform:rotate(0deg);
    transform:rotate(0deg);
  }
  100%{
    -webkit-transform:rotate(360deg);
    transform:rotate(360deg);
    }
}
