/* pages/fenxiang/fenxiang.wxss */
page{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
view{
  width: 100%;
  height: 100%;
}
image{
  width: 100%;
  height: 100%;
}
page{
  background-color: rgb(11,31,30);
}
.img_item {
  width: 48%;
  margin: 1%;
  display: inline-block;
  vertical-align: top;
}
.top{
  width: 100%;
  height: 12%;
  background-color: rgba(0, 0, 0, 0.65); 
  position: fixed;
  top: -5rpx;
  z-index: 10;
}
.back{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 50%;
  left: 40rpx;
}
.bgk{
  position: fixed;
  z-index: -1;
  width: 100%;
  height: 100%;
  bottom:0;
  filter: blur(40rpx);
}
.baocun{
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 280rpx;
  width: 280rpx;
  height: 60rpx;
  border-radius: 50rpx;
  font-size: 25rpx;
  line-height: 60rpx;
  background-color: #08A070;
  color: white;
}
.yaoqing{
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 200rpx;
  width: 280rpx;
  height: 60rpx;
  border-radius: 50rpx;
  font-size: 25rpx;
  line-height: 60rpx;
  color: #08A070;
}
.fanhui{
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 100rpx;
  font-size: 24rpx;
  display: -webkit-box !important;
  width: 100rpx;
  color: white;
}
.ewm{
  position: fixed;
  width: 600rpx;
  height: 733.7rpx;
  background-color: white;
  border-radius: 30rpx;
  left: 0;
  right: 0;
  bottom: 30%;
  margin: auto;
  box-shadow: 0 20rpx 80rpx #000;
}
.ewm .one{
  width: 100%;
  height: 100%;
  border-radius: 30rpx;
}
