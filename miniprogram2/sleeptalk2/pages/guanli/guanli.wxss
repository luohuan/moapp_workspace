/* pages/guanli/guanli.wxss */
page{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
view{
  width: 100%;
  height: 100%;
}
image{
  width: 100%;
  height: 100%;
}
page{
  background-color: rgb(11,31,30);
}
.img_item {
  width: 48%;
  margin: 1%;
  display: inline-block;
  vertical-align: top;
}
.top{
  width: 100%;
  height: 12%;
  background-color: rgba(0, 0, 0, 0.65); 
  position: fixed;
  top: -5rpx;
  z-index: 10;
}
.back{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 50%;
  left: 40rpx;
}
.bgk{
  position: fixed;
  z-index: -1;
  width: 100%;
  height: 100%;
  bottom:0;
  filter: blur(40rpx);
}
.list{
  width: 100%;
  height: 100%;
  /* background-color: #999;  */
  display: flex;
  flex-flow: column;
}
.list .one{
  width: 100%;
  display: flex; 
  flex-flow: row; 
  min-height: 180rpx;
  height: auto;
  margin-left: 10%;
}
.time{
  position: absolute;
  text-align: center;
  color: #eee;
  font-size: 20rpx;
  width: 80%;
  margin-top: -30rpx;
}
.list .one .iss{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.list .one .two{
  width: auto;
  min-height: 130rpx;
  /* background-color: #ccc; */
  display: flex;
  flex-flow: column;
}
.list .one .two .name{
  font-size: 20rpx;
  display: -webkit-box !important;
  margin: 0 0 10rpx 20rpx;
}
.list .one .three{
  width: auto;
  background-color: #0FA3DD;
  border-radius: 25rpx;
  margin-left: 20rpx;
  min-height: 70rpx;
}
.list .one .two text{
  font-size: 25rpx;
  color: #eee;
  display: -webkit-box !important;
  margin: 15rpx;
  max-width: 350rpx;
}
.fenxiang{
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 430rpx;
  width: 280rpx;
  height: 60rpx;
  border-radius: 50rpx;
  font-size: 25rpx;
  line-height: 60rpx;
  background-color: #08A070;
  color: white;
}
.huayu{
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  top: 500rpx;
  width: 400rpx;
  height: 60rpx;
  border-radius: 50rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  color: white;
}
.huayu2{
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  top: 630rpx;
  width: 400rpx;
  height: 60rpx;
  border-radius: 50rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  color: white;
}
.choicePoint{
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
  background-color: #ccc;
  margin-right: 40rpx;
  margin-top: 20rpx;
}
.bottomBtn{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 140rpx;
  background-color: rgb(27,53,52);
  border-radius: 50% 50% 0 0;
  box-shadow: 0 0 40rpx #000;
  display: flex;
  flex-flow: row;
}
.bottomBtn .delete{
  width: 120rpx;
  height: 50rpx;
  font-size: 20rpx;
  line-height: 50rpx;
  background-color: red;
  color: white;
  margin-top: 50rpx;
}
.bottomBtn .quanxuan{
  width: 120rpx;
  height: 50rpx;
  font-size: 20rpx;
  line-height: 50rpx;
  background-color: wheat;
  color: black;
  margin-top: 50rpx;
  margin-left: -20rpx;
  margin-right: -20rpx;
}
.bottomBtn .quxiao{
  width: 120rpx;
  height: 50rpx;
  font-size: 20rpx;
  line-height: 50rpx;
  background-color: white;
  color: black;
  margin-top: 50rpx;
}
.gl{
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  right: 50rpx;
  bottom: 180rpx;
  background-color: wheat;
  box-shadow: 0 10rpx 30rpx #000;
  border-radius: 50%;
  display: -webkit-flex;  
  align-content: center;
  justify-content: center;
}
.gl text{
  color:black;
  font-size: 26rpx;
  line-height: 100rpx
}
.pay{
  width: 50rpx;
  height: 50rpx;
  margin-left: 30rpx;
  margin-top: 46rpx;
}
.mengceng{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0;
  top: 0;
  left: 0;
  z-index: 5;
}
.chuangkou{
  position: fixed;
  width: 500rpx;
  height: 400rpx;
  background-color: white;
  border-radius: 10rpx;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  opacity: 0;
  z-index: 6;
}
.chuangkou .title{
  font-size: 38rpx;
  position: absolute;
  width: 100%;
  top: 8.75%;
  text-align: center;
}
.chuangkou .neirong{
  font-size: 29rpx;
  left: 0;
  right: 0;
  margin: 120rpx auto;
  position: absolute;
  width: 450rpx;
  white-space:pre;
}
.chuangkou .xuanzekuang{
  width: 30rpx;
  height: 30rpx;
  border-radius: 5rpx;
  border: 1rpx solid #ccc;
  position: absolute;
  left: 40rpx;
  top:240rpx; 
}
.chuangkou .xuanzekuang .gou{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.chuangkou .tishi{
  position: absolute;
  color: #999;
  top: 240rpx;
  left: 100rpx;
  font-size: 25rpx;
}
.chuangkou .quxiao{
  width: 47%;
  height: 70rpx;
  border-radius: 10rpx;
  position: absolute;
  bottom: 10rpx;
  left: 10rpx;
  line-height: 70rpx;
  font-size: 27rpx;
}
.chuangkou .queren{
  width: 47%;
  height: 70rpx;
  border-radius: 10rpx;
  position: absolute;
  bottom: 10rpx;
  right: 10rpx;
  /* background-color: #08A070; */
  color: white;
  line-height: 70rpx;
  font-size: 27rpx;
}
@keyframes mengceng-open{
  0%{
    opacity: 0
  }
  100%{
    opacity: 0.4
  }
}
@keyframes mengceng-close{
  0%{
    opacity: 0.4
  }
  100%{
    opacity: 0
  }
}
.c-mengceng-open{
  animation: mengceng-open 0.2s linear forwards; 
}
.c-mengceng-close{
  animation: mengceng-close 0.2s linear forwards; 
}
@keyframes chuangkou-open{
  0%{
    opacity: 0
  }
  100%{
    opacity: 1
  }
}
@keyframes chuangkou-close{
  0%{
    opacity: 1
  }
  100%{
    opacity: 0
  }
}
.c-chuangkou-open{
  animation: chuangkou-open 0.2s linear forwards; 
}
.c-chuangkou-close{
  animation: chuangkou-close 0.2s linear forwards; 
}
