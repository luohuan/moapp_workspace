  width: 430rpx;
  line-height: 50rpx;
  color: #555;
}
.xuzhi .queding{
  width: 450rpx;
  height: 80rpx;
  position: absolute;
  bottom: 20rpx;
  left: 0;
  right: 0;
  margin: auto;
  line-height: 80rpx;
  background-color: #08A070;
  color: white;
  font-size: 28rpx;
}
